package com.tigres810.adventurermod.blocks;

import com.tigres810.adventurermod.blocks.block.BlockBCTile_Neptune;
import com.tigres810.adventurermod.blocks.block.TileBC_Neptune;

import net.minecraft.block.material.Material;
import net.minecraft.block.state.IBlockState;
import net.minecraft.world.World;

public class BlockPump extends BlockBCTile_Neptune {
    public BlockPump(Material material, String id) {
        super(material, id);
    }

    @Override
    public TileBC_Neptune createTileEntity(World worldIn, IBlockState state) {
        return new TilePump();
    }
}
